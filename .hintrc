{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 130",
    "not firefox <= 130",
    "not opera <= 113",
    "not firefox <= 132",
    "not opera <= 114"
  ],
